{
    "name": "tennis-webapp",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
        "lint:eslint": "eslint . --fix",
        "lint": "run-s lint:*",
        "format": "prettier --write src/",
        "start": "npx serve -s dist -p ${PORT:-5173}"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.3",
        "axios": "^1.8.4",
        "chart.js": "^4.4.0",
        "pinia": "^3.0.2",
        "tailwindcss": "^4.1.3",
        "vue": "^3.5.13",
        "vue-chartjs": "^5.3.2",
        "vue-router": "^4.5.0",
        "vue-tg": "^0.9.0-beta.3",
        "serve": "^14.2.1"
    },
    "devDependencies": {
        "@iconify/vue": "^4.3.0",
        "@tsconfig/node22": "^22.0.1",
        "@types/node": "^22.14.0",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.0",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.24.0",
        "eslint-plugin-vue": "~10.0.0",
        "jiti": "^2.4.2",
        "npm-run-all2": "^7.0.2",
        "prettier": "3.5.3",
        "typescript": "~5.8.3",
        "vite": "^6.2.5",
        "vite-plugin-vue-devtools": "^7.7.2",
        "vue-tsc": "^2.2.8"
    }
}
